   "source": [
    "num=int(input(\"Enter a number: \"))# we give a input\n",
    "f=1 #It initializes a variable f to 1.\n",
    "for i in range(1,num+1):\n",
    "  f=f*i #This loop calculates the factorial\n",
    "print(\"Factorial is: \",f) #print Factorial"
