   ],
   "source": [
    "list_m=[3,4,5,6,7,9]\n",
    "fact=[]\n",
    "for num in list_m:\n",
    "  list_p=1\n",
    "  for i in range(1,num+1):\n",
    "    list_p *=i\n",
    "  fact.append(list_p)\n",
    "print(fact)"
   ]
